{
  "name": "joplin-logseq-exporter",
  "version": "1.0.0",
  "description": "Export Joplin notes to Logseq-compatible formats (JSON, EDN, OPML)",
  "author": "Your Name",
  "homepage": "https://github.com/yourusername/joplin-logseq-exporter",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yourusername/joplin-logseq-exporter.git"
  },
  "keywords": [
    "joplin-plugin",
    "logseq",
    "export",
    "edn",
    "json",
    "opml"
  ],
  "scripts": {
    "build": "webpack --joplin-plugin-config buildMain && webpack --joplin-plugin-config buildExtraScripts",
    "start": "webpack --joplin-plugin-config buildMain --watch",
    "update": "npm install -g generator-joplin && yo joplin --update"
  },
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^14.0.14",
    "chalk": "^4.1.0",
    "copy-webpack-plugin": "^6.1.0",
    "fs-extra": "^9.0.1",
    "glob": "^7.1.6",
    "on-build-webpack": "^0.1.0",
    "tar": "^6.0.5",
    "ts-loader": "^7.0.5",
    "typescript": "^3.9.3",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11",
    "@joplin/lib": "^2.9.0"
  },
  "dependencies": {
    "edn-data": "^0.2.0",
    "uuid": "^8.3.2"
  }
} 