{
  "id": "com.verioussmith.joplin-logseq-exporter",
  "manifest_version": 3,
  "app_min_version": "2.1",
  "version": "1.0.0",
  "name": "Logseq Exporter",
  "description": "Export Joplin notes to Logseq-compatible formats (JSON, EDN, OPML)",
  "author": "Verious Smith III",
  "homepage_url": "https://github.com/verioussmith/joplin-logseq-exporter",
  "repository_url": "https://github.com/verioussmith/joplin-logseq-exporter",
  "keywords": ["logseq", "export", "edn", "json", "opml"],
  "categories": ["exporter", "productivity"],
  "icons": {
    "16": "icons/icon-16.png",
    "32": "icons/icon-32.png",
    "48": "icons/icon-48.png"
  },
  "targets": [
    {
      "name": "desktop",
      "platforms": ["darwin", "linux", "win32"],
      "apiVersion": 26,
      "capabilities": {
        "commands": true,
        "menu": true,
        "toolbar": true,
        "panels": true,
        "settings": true
      }
    }
  ],
  "api_version": 26
} 